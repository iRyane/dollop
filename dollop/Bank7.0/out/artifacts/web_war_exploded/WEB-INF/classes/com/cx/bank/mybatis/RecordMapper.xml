<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cx.bank.dao.RecordMapper">
    <insert id="addRecord" parameterType="com.cx.bank.mybatis.Record">
        INSERT INTO RECORD(name,records,time) VALUES (#{name},#{records},#{time});
    </insert>
</mapper>