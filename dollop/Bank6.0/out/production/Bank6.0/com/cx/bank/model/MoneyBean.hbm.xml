<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cx.bank.model.MoneyBean" table="money">
        <id name="id">
            <generator class="uuid"/>
        </id>
        <property name="money"/>
    </class>

</hibernate-mapping>